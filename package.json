{
	"main": "bin/extension.js",
	"name": "proseur",
	"description": "proseur",
	"repository": {
		"url": "",
		"type:": "git"
	},
	"homepage": "",
	"categories": [],
	"extensionPack": [],
	"dependencies": {
		"mic": "^2.1.2",
		"jsdom": "^22.1.0"
	},
	"publisher": "kiss-lang",
	"contributes": {
		"keybindings": [
			{
				"command": "proseur.stopRecording",
				"mac": "Cmd+; Cmd+d",
				"key": "Ctrl+; Ctrl+d"
			},
			{
				"command": "proseur.startRecording",
				"mac": "Cmd+; Cmd+r",
				"key": "Ctrl+; Ctrl+r"
			}
		],
		"commands": [
			{
				"title": "proseur: stop transcribing",
				"command": "proseur.stopRecording"
			},
			{
				"title": "proseur: start transcribing",
				"command": "proseur.startRecording"
			}
		],
		"configuration": {
			"title": "proseur",
			"properties": {
				"proseur.whisperPath": {
					"type": "string",
					"default": ""
				}
			}
		}
	},
	"engines": {
		"vscode": "^1.4.0"
	},
